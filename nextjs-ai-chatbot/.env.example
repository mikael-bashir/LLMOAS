# Example .env file

# Database connection string
POSTGRES_URL="postgres://username:password@host:port/database?sslmode=require"

# Auth secret (generate a random secret: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`)
AUTH_SECRET="your_auth_secret_here"

# xAI API Key (get from https://console.x.ai/)
XAI_API_KEY="your_xai_api_key_here"

# Vercel Blob Store token (see https://vercel.com/docs/storage/vercel-blob)
BLOB_READ_WRITE_TOKEN="your_blob_token_here"

# Google Generative AI API Key
GOOGLE_GENERATIVE_AI_API_KEY="your_google_genai_api_key_here"

# Gemini API Key
GEMINI_API_KEY="your_gemini_api_key_here"

# Flask app entrypoint
FLASK_APP="app.api.index:app"

# Redis connection string
REDIS_URL="redis://username:password@host:port"
